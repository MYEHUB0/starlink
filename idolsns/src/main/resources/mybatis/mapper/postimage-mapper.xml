<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="postImage">
	
	<insert id="add">
		insert into post_image(attachment_no, post_no)
		values(#{attachmentNo}, #{postNo})
	</insert>
	
</mapper>