<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="simpleProfile">

	<!-- 조회 -->
	<select id="profile" resultType="MemberSimpleProfileTempDto">
		select * from member_simple_profile_temp where member_id in
		<foreach collection="list" item="memberId" open="(" close=")" separator=",">
			#{memberId}
		</foreach>
	</select>
	
</mapper>
    